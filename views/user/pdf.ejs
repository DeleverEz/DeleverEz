<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
  <title>DeliverEz &#8211; Every delivery made easier</title>
  <link rel="shortcut icon" href="assets/img/favicon.png">
  <link rel="stylesheet" href="assets/css/pdfstyle.css">
  <link rel="stylesheet" href="assets/css/pdfcss2.css">
</head>

<body>
  
  <div class="cs-container">
    <div class="cs-invoice cs-style1">
      <div class="cs-invoice_in" id="download_section">
        <div class="cs-invoice_head cs-type1 cs-mb25">
          <div class="cs-invoice_right cs-text_right">
            <div class="cs-logo cs-mb5"><img src="assets/img/logo-12.png" alt="Logo"></div>
          </div>
          <div class="cs-invoice_left">
            <p class="cs-invoice_number cs-primary_color cs-mb0 cs-f16"><b class="cs-primary_color">Invoice No:</b> <%= bookcod.orderid %></p>
          </div>
        </div>
        <div class="cs-invoice_head cs-mb10">
          <div class="cs-invoice_left">
            <b class="cs-primary_color">Shipped By:</b>
            <p>Shipper's Name: <%= user.username %><br />
              Shipper's Phone No: <%= user.phoneno %><br /></p>
          </div>
          <div class="cs-invoice_right cs-text_right">
            <b class="cs-primary_color">Shipped To:</b>
            <p>Receiver's Name: <%= bookcod.customername %><br>
              Receiver's Phone No: <%= bookcod.customerphoneno %><br>
            </p>
          </div>
        </div>
        <div class="cs-table cs-style1">
          <div class="cs-round_border">
            <div class="cs-table_responsive">
              <table >
                <thead>
                  <tr>
                    <th class="cs-width_6 cs-semi_bold cs-primary_color cs-focus_bg cs-f16" colspan="2">Shipment Details</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Tracking ID: </b><%= bookcod.orderid %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Departure Date: </b><%= bookcod.date %></td>
                  </tr>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Departure City: </b><%= user.usercity %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Arrival City: </b><%= bookcod.destinationcity %></td>
                  </tr>
                </tbody>
              </table>
              <table >
                <thead>
                  <tr>
                    <th class="cs-semi_bold cs-primary_color cs-focus_bg cs-f16 cs-border_top" colspan="4">Package Information</th>
                  </tr>
                  <tr>
                    <td class="cs-width_3 cs-primary_color cs-semi_bold">Weight</td>
                    <td class="cs-width_3 cs-primary_color cs-semi_bold">Quantity</td>
                    <td class="cs-width_3 cs-primary_color cs-semi_bold">Package Details</td>
                    <td class="cs-width_3 cs-primary_color cs-semi_bold cs-text_right">Remarks</td>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="cs-width_3"><%= bookcod.weight %> Kg</td>
                    <td class="cs-width_3"></td>
                    <td class="cs-width_3"></td>
                    <td class="cs-width_3"></td>
                  </tr>
                </tbody>
              </table>
              <table >
                <thead>
                  <tr>
                    <th class="cs-width_6 cs-semi_bold cs-primary_color cs-focus_bg cs-f16 cs-border_top" colspan="5">Payment Information</th>
                  </tr>
                  <tr class=" cs-border_top">
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">Order Type</th>
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">COD Amount</th>
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">Seller Account</th>
                    <th class="cs-width_3 cs-text_right cs-semi_bold cs-primary_color">Total</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="cs-width_3"><%= bookcod.ordertype %></td>
                    <td class="cs-width_3">Rs: <%= bookcod.deliverycharges %></td>
                    <td class="cs-width_3"><%= user.sellerid %></td>
                    <td class="cs-width_3">Rs: <%= bookcod.deliverycharges %></td>
                  </tr>
                </tbody>
              </table>
            </div>
            <!-- <div class="cs-invoice_footer cs-border_top">
              <div class="cs-left_footer cs-mobile_hide"></div>
              <div class="cs-right_footer">
                <table>
                  <tbody>
                    <tr class="cs-border_left">
                      <td class="cs-width_3 cs-semi_bold cs-primary_color cs-focus_bg">Subtoal</td>
                      <td class="cs-width_3 cs-semi_bold cs-primary_color cs-focus_bg cs-primary_color cs-text_right">$1420</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div> -->
          </div>
          <div class="cs-invoice_footer">
            <div class="cs-left_footer cs-mobile_hide"></div>
            <div class="cs-right_footer">
              <table>
                <tbody>
                  <tr class="cs-border_none">
                    <td class="cs-width_3 cs-border_top_0 cs-bold cs-f16 cs-primary_color">Total Amount</td>
                    <td class="cs-width_3 cs-border_top_0 cs-bold cs-f16 cs-primary_color cs-text_right">Rs: <%= bookcod.deliverycharges %></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="cs-table cs-style1" style="display: none;">
          <div class="cs-round_border">
            <div class="cs-table_responsive" id="printPageArea">
              <table >
                <thead>
                  <tr>
                    <th class="cs-width_6 cs-semi_bold cs-primary_color cs-focus_bg cs-f16" colspan="2">Shipment Details</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Seller Account: </b><%= user.sellerid %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Tracking ID: </b><%= bookcod.orderid %></td>
                  </tr>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Departure Date: </b><%= bookcod.date %></td>
                    <td class="cs-width_6"><b class="cs-primary_color"></td>
                  </tr>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Shipper's Name: </b><%= user.username %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Receiver's Name: </b><%= bookcod.customername %></td>
                  </tr>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Shipper's Phone No: </b><%= user.phoneno %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Receiver's Phone No: </b><%= bookcod.customerphoneno %></td>
                  </tr>
                  <tr>
                    <td class="cs-width_6"><b class="cs-primary_color">Shipper's City: </b><%= user.usercity %></td>
                    <td class="cs-width_6"><b class="cs-primary_color">Receiver's City: </b><%= bookcod.destinationcity %></td>
                  </tr>
                  
                </tbody>
              </table>
              <table id="printPageArea">
                <thead>
                  <tr>
                    <th class="cs-width_6 cs-semi_bold cs-primary_color cs-focus_bg cs-f16 cs-border_top" colspan="5">Payment Information</th>
                  </tr>
                  <tr class=" cs-border_top">
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">Order Type</th>
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">Weight</th>
                    <th class="cs-width_3 cs-semi_bold cs-primary_color">COD Amount</th>
                    <th class="cs-width_3 cs-text_right cs-semi_bold cs-primary_color">Total</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="cs-width_3"><%= bookcod.ordertype %></td>
                    <td class="cs-width_3"><%= bookcod.weight %> Kg</td>
                    <td class="cs-width_3">Rs: <%= bookcod.deliverycharges %></td>
                    <td class="cs-width_3"><%= bookcod.deliverycharges %></td>
                  </tr>
                </tbody>
              </table>
            </div>
            <!-- <div class="cs-invoice_footer cs-border_top">
              <div class="cs-left_footer cs-mobile_hide"></div>
              <div class="cs-right_footer">
                <table>
                  <tbody>
                    <tr class="cs-border_left">
                      <td class="cs-width_3 cs-semi_bold cs-primary_color cs-focus_bg">Subtoal</td>
                      <td class="cs-width_3 cs-semi_bold cs-primary_color cs-focus_bg cs-primary_color cs-text_right">$1420</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div> -->
          </div>
          <div class="cs-invoice_footer">
            <div class="cs-left_footer cs-mobile_hide"></div>
            <div class="cs-right_footer">
              <table>
                <tbody>
                  <tr class="cs-border_none">
                    <td class="cs-width_3 cs-border_top_0 cs-bold cs-f16 cs-primary_color">Total Amount</td>
                    <td class="cs-width_3 cs-border_top_0 cs-bold cs-f16 cs-primary_color cs-text_right">Rs: <%= bookcod.deliverycharges %></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="cs-note">
          <div class="cs-note_left">
            <svg xmlns="http://www.w3.org/2000/svg" class="ionicon" viewBox="0 0 512 512"><path d="M416 221.25V416a48 48 0 01-48 48H144a48 48 0 01-48-48V96a48 48 0 0148-48h98.75a32 32 0 0122.62 9.37l141.26 141.26a32 32 0 019.37 22.62z" fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="32"/><path d="M256 56v120a32 32 0 0032 32h120M176 288h160M176 368h160" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32"/></svg>
          </div>
          <div class="cs-note_right">
            <p class="cs-mb0"><b class="cs-primary_color cs-bold">Note:</b></p>
            <p class="cs-m0">Please refer to our website to terms and conditions.</p>
          </div>
          <div class="cs-note_left" style="padding-left: 25%;">
            <p class="cs-mb0"><b class="cs-primary_color cs-bold">Signature:</b></p>
            <p class="cs-m0">---------------.</p>
          </div>
        </div><!-- .cs-note -->
      </div>
      <div class="cs-invoice_btns cs-hide_print">
        <a href="javascript:window.print()" class="cs-invoice_btn cs-color1">
          <svg xmlns="http://www.w3.org/2000/svg" class="ionicon" viewBox="0 0 512 512"><path d="M384 368h24a40.12 40.12 0 0040-40V168a40.12 40.12 0 00-40-40H104a40.12 40.12 0 00-40 40v160a40.12 40.12 0 0040 40h24" fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="32"/><rect x="128" y="240" width="256" height="208" rx="24.32" ry="24.32" fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="32"/><path d="M384 128v-24a40.12 40.12 0 00-40-40H168a40.12 40.12 0 00-40 40v24" fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="32"/><circle cx="392" cy="184" r="24"/></svg>
          <span>Download PDF</span>
        </a>
        <button onclick="printPageArea('printPageArea');" type="submit" class="cs-invoice_btn cs-color2">Print</button>
        <!-- <button onclick="printPageArea('printPageArea');"  class="cs-invoice_btn cs-color2">
          <svg xmlns="http://www.w3.org/2000/svg" class="ionicon" viewBox="0 0 512 512"><title>Download</title><path d="M336 176h40a40 40 0 0140 40v208a40 40 0 01-40 40H136a40 40 0 01-40-40V216a40 40 0 0140-40h40" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32"/><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" d="M176 272l80 80 80-80M256 48v288"/></svg>
          <span>Download</span>
        </button> -->
      </div>
    </div>
  </div>
  <script src="assets/js/pdfjquery.min.js"></script>
  <script src="assets/js/pdfjs.min.js"></script>
  <script src="assets/js/pdfhtml2canvas.min.js"></script>
  <script src="assets/js/pdfmain.js"></script>
  <script>
    function printPageArea(tagid){
        var hashid = "#"+ tagid;
        var tagname =  $(hashid).prop("tagName").toLowerCase() ;
        var attributes = ""; 
        var attrs = document.getElementById(tagid).attributes;
          $.each(attrs,function(i,elem){
            attributes +=  " "+  elem.name+" ='"+elem.value+"' " ;
          })
        var divToPrint= $(hashid).html() ;
        var head = "<html><head>"+ $("head").html() + "</head>" ;
        var allcontent = head + "<body  onload='window.print()' >"+ "<" + tagname + attributes + ">" +  divToPrint + "</" + tagname + ">" +  "</body></html>"  ;
        var newWin= window.open('', '', 'left=0,top=0,width=1000,height=900,toolbar=0,scrollbars=0,status=0');
        newWin.document.open();
        newWin.document.write(allcontent);
        newWin.document.close();
    }
  </script>
</body>
</html>